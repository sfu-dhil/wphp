{% embed 'partial/table.html.twig' %}
    {% set entities = title_relationships %}
    {% block thead %}
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Label</th>
            <th>Description</th>

        </tr>
    {% endblock %}
    {% block tbody %}
        {% for title_relationship in title_relationships %}
            <tr>
                <td>
                    <a href="{{ path('title_relationship_show', { 'id': title_relationship.id }) }}">
                        {{ title_relationship.id }}
                    </a>
                </td>
                <td>
                    {{ title_relationship.name }}
                </td>

                <td>
                    {{ title_relationship.label }}
                </td>

                <td>
                    {{ title_relationship.description|raw }}
                </td>

            </tr>
        {% endfor %}
    {% endblock %}
{% endembed %}
  