{% apply spaceless %}
    {% if person.gender == constant('App\\Entity\\Person::FEMALE') %}
        Female
    {% elseif person.gender == constant('App\\Entity\\Person::MALE') %}
        Male
    {% elseif person.gender == constant('App\\Entity\\Person::TRANS') %}
        Transgender
    {% elseif person.gender == constant('App\\Entity\\Person::UNKNOWN') %}
        Unknown
    {% elseif person.gender == "" %}
        Unknown
    {% else %}
        Unknown ({{ person.gender }})
    {% endif %}
{% endapply %}
