{% apply spaceless %}
    {% if firm.gender == constant('App\\Entity\\Person::FEMALE') %}
        Female
    {% elseif firm.gender == constant('App\\Entity\\Person::MALE') %}
        Male
    {% elseif firm.gender == constant('App\\Entity\\Person::UNKNOWN') %}
        Unknown
    {% else %}
        Unknown ({{ firm.gender }})
    {% endif %}
{% endapply %}
